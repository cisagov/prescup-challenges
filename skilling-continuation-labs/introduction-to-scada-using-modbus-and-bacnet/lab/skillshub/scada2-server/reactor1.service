[Unit]
Description=scada console server
After=network.target

[Service]
User=root
WorkingDirectory=/home/user/Documents/
ExecStart= /usr/bin/python3 /home/user/Documents/reactor_server_1.py
Restart=always
RestartSec=3

[Install]
WantedBy=multi-user.target


